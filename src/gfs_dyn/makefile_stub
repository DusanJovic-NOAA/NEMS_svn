##  This is an elementary grid component or say independent grid component ##
#
# *****************************************************************
#
include ../../ush/compile_settings
MAKEFILE=makefile_stub
TARGET = gfs_dyn_stub.a
OBJS0	= \
	  gfs_dynamics_grid_comp_mod_stub.o
	  
INCS =  \
	$(INCDIR)/f_hpm.h mpi_inc.h $(INCDIR)/function2

#
# *****************************************************************
#

.SUFFIXES: .o .F90

default:
	@gmake -f $(MAKEFILE) all

all: $(LIBDIR)/libstub/$(TARGET)


$(LIBDIR)/libstub/$(TARGET):  $(OBJS0)  
	$(AR) $(ARFLAGS) $@ $?

clean:
	/bin/rm -f $(OBJS0) 
	/bin/rm -f *.mod 
	/bin/rm -f *.o
	/bin/rm -f $(LIBDIR)/libstub/$(TARGET)

.F90.o:
	$(F77) $(FFLAGS) -c $*.F90

gfs_dynamics_grid_comp_mod_stub.o:	 gfs_dynamics_grid_comp_mod_stub.F90
		$(F77) $(FFLAG90) -c gfs_dynamics_grid_comp_mod_stub.F90

