	$(F77) $(FFLAG90) -c GEFS_Cpl_Run_ESMFMod_STUB.f

GEFS_Cpl_ESMFMod_STUB.o: GEFS_Cpl_ESMFMod_STUB.f
	$(F77) $(FFLAG90) -c GEFS_Cpl_ESMFMod_STUB.f

