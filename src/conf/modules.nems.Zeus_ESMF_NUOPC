#!/bin/sh

# This script is responsible for loading modules that are compatible with
# the NUOPC Layer version used in NEMS. ESMF 6.2.0 is currently the official
# reference release of the NUOPC Layer.

module purge
#module load mpt intel esmf/6.2.0
# -> MOM5 needs a NetCDF enabled ESMF -> for now use ESMF under my homedir
module use /home/Gerhard.Theurich/Modulefiles
module load mpt intel esmf/6.3.0bs04-netcdf3.6.3
