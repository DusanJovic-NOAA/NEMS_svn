#### this is elementary grid component or independent grid component ####
#---------------
# For non-debug.
#---------------
#
include ../../ush/compile_settings
MAKEFILE=makefile_stub
TARGET= gfs_phys_stub.a
# *****************************************************************
#
OBJS0   = \
		machine.o \
		ozne_def.o \
		layout1.o \
		date_def.o \
		namelist_physics_def.o \
		resol_def.o \
		mersenne_twister.o \
		physcons_v.o \
		funcphys_v.o \
		iounitdef.o \
		radiation_astronomy.o \
		radiation_gases.o \
		module_bfmicrophysics.o \
		gfs_physics_grid_comp_mod_stub.o

#
# *****************************************************************
#

.SUFFIXES: .o .f

default:
	@gmake -f $(MAKEFILE) all

all: $(LIBDIR)/libstub/$(TARGET)

$(LIBDIR)/libstub/$(TARGET):  $(OBJS0) 
	$(AR) $(ARFLAGS) $@ $?

clean:
	/bin/rm -f $(OBJS0) 
	/bin/rm -f *.mod 
	/bin/rm -f *.o
	/bin/rm -f $(LIBDIR)/libstub/$(TARGET)

.f.o:
	$(F77) $(FFLAGS) -c $*.f

#
# *****************************************************************
#
machine.o : machine.f
		$(F77) $(FFLAG90) -c machine.f
ozne_def.o: ozne_def.f
		$(F77) $(FFLAG90) -c ozne_def.f
namelist_physics_def.o: namelist_physics_def.f
		$(F77) $(FFLAG90) -c namelist_physics_def.f
layout1.o: layout1.f
		$(F77) $(FFLAG90) -c layout1.f
date_def.o: date_def.f
		$(F77) $(FFLAG90) -c date_def.f
mersenne_twister.o: mersenne_twister.f
		$(F77) $(FFLAG90) -c mersenne_twister.f
resol_def.o: resol_def.f
		$(F77) $(FFLAG90) -c resol_def.f
physcons_v.o: physcons_v.f
		$(F77) $(FFLAG90) -c physcons_v.f
funcphys_v.o: funcphys_v.f
		$(F77) $(FFLAG90) -c funcphys_v.f
iounitdef.o: iounitdef.f
		$(F77) $(FFLAG90) -c iounitdef.f
radiation_astronomy.o: radiation_astronomy.f
		$(F77) $(FFLAG90) -c radiation_astronomy.f
radiation_gases.o: radiation_gases.f
		$(F77) $(FFLAG90) -c radiation_gases.f
module_bfmicrophysics.o: module_bfmicrophysics.f
		$(F77) $(FFLAG90) -c module_bfmicrophysics.f
gfs_physics_grid_comp_mod_stub.o:    gfs_physics_grid_comp_mod_stub.f
		$(F77) $(FFLAG90) -c gfs_physics_grid_comp_mod_stub.f
                                                                                
