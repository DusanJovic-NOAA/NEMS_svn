include ../../ush/compile_settings
MAKEFILE=makefile_stub
TARGET= gfs_phys_stub.a
# *****************************************************************
#
OBJS0   = \
		ozinterp_stub.o \
		compns_physics_stub.o \
		set_soilveg_stub.o \
		gfuncphys_stub.o \
		sfccycle_stub.o \
		gbphys_v_stub.o \
		layout1_stub.o \
		date_def_stub.o \
		namelist_physics_def_stub.o \
		resol_def_stub.o \
		mersenne_twister_stub.o \
		physcons_v_stub.o \
		funcphys_v_stub.o \
		radiation_astronomy_stub.o \
		tracer_const_h_stub.o \
		grrad_stub.o \
		gfs_physics_grid_comp_mod_stub.o

#
# *****************************************************************
#

.SUFFIXES: .o .f

default:
	@gmake -f $(MAKEFILE) all

all: $(LIBDIR)/libstub/$(TARGET)

$(LIBDIR)/libstub/$(TARGET):  $(OBJS0) 
	$(AR) $(ARFLAGS) $@ $?

clean:
	/bin/rm -f $(OBJS0) 
	/bin/rm -f *.mod 
	/bin/rm -f *.o
	/bin/rm -f $(LIBDIR)/libstub/$(TARGET)

$(OBJS0): %.o: %.f
	$(F77) $(FFLAG90) -c $*.f

