##  This is an elementary grid component or say independent grid component ##
#---------------
# For non-debug.
#---------------
#
include ../../ush/compile_settings
MAKEFILE=makefile_stub
TARGET= gfs_io_stub.a
# *****************************************************************
#
OBJ_IO	= \
	  module_WRITE_ROUTINES_GFS_STUB.o         \
	  module_WRITE_GRID_COMP_GFS_STUB.o 



INCS = \
		$(INCDIR)/f_hpm.h mpi_inc.h $(INCDIR)/function2


# *****************************************************************
#

.SUFFIXES: .o .f90

default:
	@gmake -f $(MAKEFILE) all

all: $(LIBDIR)/libstub/$(TARGET)

$(LIBDIR)/libstub/$(TARGET):  $(OBJ_IO) 
	$(AR) $(ARFLAGS) $@ $?  

clean:
	/bin/rm -f $(OBJS_IO) 
	/bin/rm -f *.mod 
	/bin/rm -f *.o
	/bin/rm -f $(LIBDIR)/libstub/$(TARGET)


$(OBJ_IO):     $(DEPS)
	$(FC) $(FFLAG_GFS_IO) -c $*.f90
